{
  "version": "2.0",
  "app_name": "multilingual-chatbot",
  "stages": {
    "dev": {
      "api_gateway_stage": "dev",
      "environment_variables": {
        "AWS_DEFAULT_REGION": "us-east-1"
      },
      "iam_role_arn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/ChaliceRole"
    },
    "prod": {
      "api_gateway_stage": "prod",
      "environment_variables": {
        "AWS_DEFAULT_REGION": "us-east-1"
      },
      "iam_role_arn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/ChaliceRole"
    }
  }
}
